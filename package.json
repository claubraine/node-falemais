{
    "name": "node-falemais",
    "version": "1.0.0",
    "description": "RESTful back-end for Node.js and Express.js, Swagger e PostgreSQL",
    "license": "UNLICENSED",
    "author": "ClaudineiBraine",
    "main": "src/index",
    "module": "commonjs",
    "repository": {
        "type": "git",
        "url": "https://github.com/claubraine/node-falemais.git"
    },
    "scripts": {
        "start": "pm2 start production.config.json",
        "start-no-daemon": "pm2 start production.config.json --no-daemon",
        "start-node": "node build/src/index.js",
        "dev": "ts-node-dev src/index.ts",
        "test": "jest -i --colors --verbose --detectOpenHandles",
        "lint": "eslint src --ext .ts",
        "build": "tsc -p tsconfig.build.json",
        "typecheck": "tsc --noEmit",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "autogen-swagger": "node swagger.js",
        "producao": "npm run autogen-swagger && npm run build && npm run start-node && exit"
    },
    "dependencies": {
        "axios": "^0.22.0",
        "bcrypt": "5.0.1",
        "body-parser": "^1.19.0",
        "chalk": "^4.1.2",
        "circular-json": "^0.5.9",
        "compression": "^1.7.4",
        "concurrently": "^6.3.0",
        "consign": "^0.1.6",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "ejs": "^3.1.6",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "express": "^4.17.1",
        "express-passport-logout": "^0.1.0",
        "joi": "^17.4.1",
        "jsonwebtoken": "^8.5.1",
        "node-fetch": "^3.0.0",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "pg": "^8.7.1",
        "pm2": "^5.1.2",
        "sequelize": "^6.6.5",
        "sha1": "^1.1.1",
        "sqlite3": "5.0.2",
        "swagger-autogen": "^2.11.2",
        "swagger-ui-express": "^4.1.4",
        "typeorm": "0.2.34",
        "wpapi": "^1.2.2",
        "wpcom": "^5.4.2",
        "xml-js": "^1.6.11",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@types/bcrypt": "5.0.0",
        "@types/circular-json": "^0.4.0",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.11",
        "@types/express": "^4.17.13",
        "@types/jest": "^26.0.24",
        "@types/node": "^16.10.3",
        "@types/passport-jwt": "^3.0.6",
        "@types/pg": "^8.6.1",
        "@types/request": "^2.48.7",
        "@types/sqlite3": "3.1.7",
        "@types/supertest": "^2.0.11",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/wpapi": "^1.1.1",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-plugin-import": "^2.23.4",
        "jest": "^27.2.5",
        "nodemon": "^2.0.13",
        "sequelize-cli": "^6.2.0",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-node": "10.1.0",
        "ts-node-dev": "1.1.8",
        "typescript": "^4.4.3"
    }
}
